            <ComboBoxItem>Square</ComboBoxItem>
            <ComboBoxItem>Triangle</ComboBoxItem>
            <ComboBoxItem>Circle</ComboBoxItem>
        </ComboBox>
        <ComboBox SelectionChanged="comboBoxTypeSelect_SelectionChanged" x:Name="comboBoxTypeSelect" Canvas.Left="341" Canvas.Top="84" Width="120" Margin="288,83,109,214">
            <ComboBoxItem>Red</ComboBoxItem>
            <ComboBoxItem>Blue</ComboBoxItem>
            <ComboBoxItem>Yellow</ComboBoxItem>
        </ComboBox>
        <Canvas Name="mainCanvas">
            


        </Canvas>
    </Grid>
</Window>
